{
  "version": 2,
  "builds": [
    {
      "src": "/backend/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "/frontend/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/main.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}